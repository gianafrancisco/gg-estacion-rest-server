<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ggingenieria.estacion.modelos">
    <class name="Empresa" table="empresa">
        <id column="empresaId" name="empresaId">
            <generator class="native"/>
        </id>
        <property name="nombre"/>
        <property name="localidad"/>
        <property name="numeroTelefono"/>
        <property name="nombreContacto"/>
        <property name="fechaAlta"/>
        <property name="descuento"/>
        <property name="vencimiento"/>
        <property name="activo"/>
    </class>
</hibernate-mapping>
