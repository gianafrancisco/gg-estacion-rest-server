<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ggingenieria.estacion.modelos">
  <class name="Registro" table="registro">
    <id column="registroId" name="registroId">
      <generator class="native"/>
    </id>
    <property name="usuarioId"/>
    <property name="nombreUsuario"/>
    <property name="nombre"/>
    <property name="apellido"/>
    <property name="permisos"/>
    <property name="surtidorId"/>
    <property name="descripcionSurtidor"/>
    <property name="direccionNodo"/>
    <property name="productoId"/>
    <property name="descripcionProducto"/>
    <property name="puntosCambiados"/>
    <property name="empresaId"/>
    <property name="nombreEmpresa"/>
    <property name="descuento"/>
    <property name="vencimiento"/>
    <property name="vehiculoId"/>
    <property name="autorizado"/>
    <property name="dominio"/>
    <property name="dominioMunicipal"/>
    <property name="fechaRegistro"/>
    <property name="fechaProximaVenta"/>
    <property name="fechaVencimiento"/>
    <property name="accion"/>
  </class>
</hibernate-mapping>
